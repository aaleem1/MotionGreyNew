                        </label>
                        {% else %}
                        <label>
                            {{ section.settings.text_product_type | split: '|' | first }}
                        </label>
                        {% endif %}
                        {% endif %}
                        <span>{{ product.type }}</span>
                    </div>
                    {% endif %}
                </div>
                {% endif %}
              
              {% assign iso-code = shop.money_with_currency_format | slice: -3, 3 %}
