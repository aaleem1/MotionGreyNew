                        {% if settings.enable_multilang %}
                        <label class="lang1">
                            {{ section.settings.text_vendor | split: '|' | first }}
                        </label>
                        <label class="lang2">
                            {{ section.settings.text_vendor | split: '|' | last }}
                        </label>
                        {% else %}
                        <label>
                            {{ section.settings.text_vendor | split: '|' | first }}
                        </label>
                        {% endif %}
