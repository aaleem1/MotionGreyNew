 }
  
  .custom-btn svg {
	left: 0;
    position: absolute;
    top: 0;
    width: 100%;
    height: 100%;
  }
  
  .custom-btn rect {
    fill: none;
    stroke: #fff;
    stroke-width: 2;
    stroke-dasharray: 422, 0;
    transition: all 0.35s linear;
  }
  
  
.custom-btn:hover {
  letter-spacing: 1px;
  text-decoration: none;
  color: #fff;
}
  
.custom-btn:hover rect {
    stroke-width: 5;
    stroke-dasharray: 15, 310;
    stroke-dashoffset: 48;
    transition: all 1.35s cubic-bezier(0.19, 1, 0.22, 1);
}
    
