.upsell-col {
    display: flex;
    flex-wrap: nowrap;
    gap: 19px;
    text-align: center;
    position: relative;
    justify-content: center;
    text-align: left;
    align-self: auto;
}
a.upsell-image {
    border: 1px solid #ccc;
}
.info-block {
    align-self: center;
}
.productTabs .header-top {
    display: none;
}
.productTabs .showsubheader .header-top {
    display: block;
}
.stamped-photos-carousel {
    padding-bottom: 20px;
}
.stamped-photos-carousel:after {
    content: "Click to see Photos";
    left: 0;
    right: 0;
    width: 100%;
    justify-content: center;
    text-align: center;
    top: 85%;
    position: absolute;
    z-index: 9;
    font-size: 14px;
}
.product-recommendations .box-title {
    font-size: 36px;
    line-height: 1.5em;
    color: #2C2C2C;
    font-weight: bold;
}
.related-products .action {
    display: none;
}
.swatch .swatch-element:not(.color) {
    margin-bottom: 5px;
}


@media(min-width: 769px){
.col-md-12 {
    -webkit-box-flex: 0;
    -ms-flex: 0 0 100%;
    flex: 0 0 100%;
    max-width: 100%;
}
.slider-nav.thumbCustomStyle img {
    max-width: 155px;
    max-height: 155px;
    width: 100%;
    height: 100%;
}
.slider-nav .item.slick-slide {
    padding: 3px;
}
}


