      @include flexbox();
      @include justify-content(space-between);
      padding-left: 34px;

      &:before {
        font-family: FontAwesome;
        content: "\f106";
        text-rendering: auto;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
        display: inline-block;
        position: absolute;
        left: 0px;
        top: 45%;
        font-weight: normal;
        font-size: 25 px;
        color: {{settings.color_product_title}};
      }
      .clear-all {
        font-size: $font_size;
        color: #ec3323;
        background: #fef1ef;
        padding: 0 8px;
        max-height: 25px;
        line-height: 25px;
        @include border-radius(1px);
        font-weight: normal;
        letter-spacing: normal;
        text-transform: capitalize;

        &:before {
          background-color: #999999;
          bottom: 2px;
        }
        &:hover{
          color:{{settings.color_product_title}};
          &:before{
            background-color: #fff;
          }
        }
      }

    }

    .widget-title {
      &.open {
        .sidebar-title:before {
          @include rotate(180);
          top: 50%;
        }
      }
    }
    .list-tags {
      input[type="checkbox"]+label:before{
            border: 1px solid #dddddd;
            @include border-radius(1px);
            width: 19px;
            height: 19px;
            top: 2px;
          }
      input[type="checkbox"]:checked + label:before{
        background: {{settings.color_product_title}};
        border-color: {{settings.color_product_title}};
      }
      input[type="checkbox"]+label:after{
        width: 10px;
        height: 5px;
        top: 7px;
        left: 5px;
        border-color: #fff;
      }
    }

    .filter-color {
      .widget-content {
        margin-bottom: 2px;
      }
      .list-tags {
        margin-left: -5px;
        margin-right: -5px;
        @include flexbox();
        @include flex-wrap(wrap);

        li {
          margin: 0 0 10px;
          padding-left: 0px;
          padding-right: 16px;
          padding-bottom: 5px;
        }

        a {
          display: inline-block;
          width: 34px;
          height: 34px;
          @include border-radius(50%);
          box-shadow: 0 0 1px 2px #e7e7e7;
          border: 2px solid #fff;
          position: relative;

          img {
            @include border-radius(50%);
            width: 30px;
            height: 30px;
            object-fit: cover;
          }
          &:hover,
          &.active {
           border-color: #e7e7e7;
          }
        }

      }
    }
    .filter-size {
      .list-tags {
        @include flexbox();
        @include flex-wrap(wrap);

        li {
          -webkit-box-flex: 0;
          -ms-flex: 0 0 50%;
          flex: 0 0 50%;
          max-width: 50%;
        }
      }
    }

